# Production Environment Configuration
# Inherits from parent .env file

# Production Security Settings
NEXT_PUBLIC_SECURITY_LEVEL=production
NEXT_PUBLIC_DISABLE_AUTH=false

# Production-specific settings
NEXT_PUBLIC_LOG_LEVEL=warn

# Security features enabled
NEXT_PUBLIC_ENABLE_DEV_TOOLS=false
NEXT_PUBLIC_SHOW_AUTH_BYPASS=false

# RLS enforcement levels (for gradual rollout)
NEXT_PUBLIC_RLS_ENFORCEMENT_LEVEL=permissive
# Options: permissive, restricted, strict